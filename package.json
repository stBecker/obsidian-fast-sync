{
  "name": "fast-sync",
  "version": "1.0.3",
  "description": "Sync your vaults across multiple devices, with end-to-end encryption and version history.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "format": "prettier --write '**/*.ts'",
    "lint": "eslint . --fix --ext .ts"
  },
  "prettier":{
    "printWidth": 140,
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "*",
    "@types/bun": "latest",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "builtin-modules": "*",
    "esbuild": "*",
    "globals": "*",
    "obsidian": "latest",
    "tslib": "*",
    "typescript": "*",
    "typescript-eslint": "*",
    "eslint-plugin-import": "*",
    "eslint":"*",
    "prettier-plugin-organize-imports": "*"
  },
  "dependencies": {
  },
  "private": true
}